{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "ESNext",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "baseUrl": "src",
        "paths": {
            "@scss/*": ["scss/*"],
            "@/*": ["*"],
            "@img/*": ["img/*"]
        }
    }
}